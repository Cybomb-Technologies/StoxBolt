<IfModule mod_rewrite.c>
  RewriteEngine On

  # Force non-www â†’ www
  RewriteCond %{HTTP_HOST} ^stoxbolt\.com$ [NC]
  RewriteRule ^(.*)$ https://www.stoxbolt.com/$1 [L,R=301]

  RewriteBase /
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^ index.html [L]
</IfModule>

